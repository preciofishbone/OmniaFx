{
  "name": "Omnia.Fx.Examples.WebAppWithDb",
  "version": "0.1.0",
  "scripts": {
    "omnia-before-build": "gulp omnia-before-build",
    "omnia-after-build": "gulp omnia-after-build",
    "test": "jest"
  },
  "dependencies": {
    "@omnia/fx-models": "2.1.1",
    "@omnia/fx-sp": "2.1.1",
    "@omnia/fx-spfx": "2.1.1",
    "@omnia/tooling-vue": "2.1.1",
    "@omnia/wcm": "2.1.0"
  },
  "devDependencies": {
    "@types/jest": "^22.1.0",
    "jest": "^22.1.4",
    "jest-cli": "^22.1.4",
    "ts-jest": "^22.0.1"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "jest": {
    "transform": {
      ".(ts|tsx|jsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "moduleNameMapper": {
      "(@omnia/fx/)": "<rootDir>/__mocks__/index.ts"
    },
    "testRegex": "(/.*|\\.(test|spec))\\.(test.ts|test.tsx)$",
    "testPathIgnorePatterns": [
      "<rootDir>/wwwroot",
      "<rootDir>/node_modules"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "testEnvironment": "node"
  }
}
